{
  "name": "@theholocron/node-template",
  "version": "1.2.1",
  "description": "<description>",
  "homepage": "https://github.com/theholocron/node-template#readme",
  "bugs": "https://github.com/theholocron/node-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/theholocron/node-template.git"
  },
  "license": "GPL-3.0",
  "author": "Newton Koumantzelis",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/node-template.es.js",
      "require": "./dist/node-template.cjs.js"
    }
  },
  "main": "dist/node-template.cjs.js",
  "module": "dist/node-template.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "docker run --rm -e RUN_LOCAL=true -v .:/tmp/lint github/super-linter",
    "lint:debug": "docker run --rm -e LOG_LEVEL=DEBUG -e RUN_LOCAL=true -v .:/tmp/lint github/super-linter",
    "prepare": "husky",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@theholocron/commitlint-config": "^4.1.0",
    "@theholocron/eslint-config": "^4.1.0",
    "@theholocron/lint-staged-config": "^4.1.0",
    "@theholocron/prettier-config": "^4.1.0",
    "@tsconfig/node-lts": "^22.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "alex": "^11.0.1",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "sort-package-json": "^3.4.0",
    "tsc-files": "^1.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@9.5.0",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./dist/**/*.js",
        "maxSize": "100 kB"
      }
    ]
  },
  "releases": "https://github.com/theholocron/node-template/releases",
  "wiki": "https://github.com/theholocron/node-template/wiki"
}
